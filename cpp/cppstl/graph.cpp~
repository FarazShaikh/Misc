#include "graph.h"

int main() {
   Graph g(8);
   g.insertEdge(0,1);
   g.insertEdge(0,4);
   g.insertEdge(0,5);
   g.insertEdge(1,2);
   g.insertEdge(1,6);
   g.insertEdge(2,3);
   g.insertEdge(3,7);
   g.insertEdge(4,5);
   g.insertEdge(5,6);
   g.insertEdge(6,7);
   g.printEdges();
   cout << "BFS" << endl;
   g.bfs();
}



/*

0 - 1 - 2 - 3
| \   \     |
4 - 5 - 6 - 7

*/
